<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wis.mapper.SysUserMapper">

<insert id="save" parameterType="com.wis.pojo.po.User" useGeneratedKeys="true" keyProperty="id">
    insert into wis_user(username,password,cname,lxdh,addtime) values(#{username},#{password},#{cname},#{lxdh},#{addtime})
</insert>

</mapper>